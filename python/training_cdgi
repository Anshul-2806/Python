a = 2
b = 4
print("Before swapping", a,b)

c = a
a = b
b = c
print("After Swapping ", a,b)

# direct swapping the two numbers
a,b = 1,2
print("Before swapping", a,b)
a,b = b,a
print("After Swapping ", a,b)

