#  nested for loop

for i in range(0,4):
    for j in range(0,4):
        print(j, end=" ")
    print()

print("Table from 1 to 10 ")
for i in range(1,11):
    for j in range(1,11):
        print(i*j, end=" ")
    print()

print("Right angle triangle")
for i in range(1,4):
    for j in range(1,i+1):
        print("*", end=" ")
    print()

print("opposite right angle triangle")
for i in range(3):
    for j in range(3-i):
        print("*", end=" ")
    print()

